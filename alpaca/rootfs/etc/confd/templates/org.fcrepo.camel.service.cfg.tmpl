# The baseUrl for the fedora repository.
fcrepo.baseUrl={{ getv "/fcrepo/url" (getenv "ALPACA_fcrepo_url") }}

# If authentication is enabled on the Fedora repository, add appropriate values here
fcrepo.authUsername={{ getv "/fcrepo/auth/user" (getenv "ALPACA_FCREPO_AUTH_USER") }}
fcrepo.authPassword={{ getv "/fcrepo/auth/password" (getenv "ALPACA_FCREPO_AUTH_PASSWORD") }}
fcrepo.authHost={{ getv "/fcrepo/auth/host" (getenv "ALPACA_FCREPO_AUTH_HOST") }}
