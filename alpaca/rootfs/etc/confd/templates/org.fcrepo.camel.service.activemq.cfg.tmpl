# The JMS connection URI, used for connecting to a local or remote ActiveMQ broker
jms.brokerUrl={{ getv "/activemq/url" (getenv "ALPACA_ACTIVEMQ_URL") }}

# If authentication is enabled on the activemq broker, add appropriate values here
jms.username={{ getv "/activemq/user" (getenv "ALPACA_ACTIVEMQ_USER") }}
jms.password={{ getv "/activemq/password" (getenv "ALPACA_ACTIVEMQ_PASSWORD") }}
